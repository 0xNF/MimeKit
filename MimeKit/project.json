{
  "version": "1.0.0-*",
  "title": "MimeKit",
  "description": "An Open Source library for creating and parsing MIME, S/MIME, PGP messages on desktop and mobile platforms.",
  "authors": [ "Jeffrey Stedfast" ],
  "tags": [ "mime", "encryption", "security", "smime", "s/mime", "openpgp", "pgp", "mbox", "email", "parser", "tnef", "net35", "net40", "net45", "corefx", "xamarin", "android", "ios", "monodroid", "monotouch", "win8", "wp81" ],
  "projectUrl": "https://github.com/jstedfast/MimeKit",
  "licenseUrl": "https://github.com/jstedfast/MimeKit/blob/master/License.md",
  "language": "en-US",

  "frameworks": {
    "net45": {
      "compilationOptions": {
        "define": [ "SERIALIZABLE", "ENABLE_CRYPTO", "ENABLE_SNM" ]
      },
      "frameworkAssemblies": {
        "System.Core": "",
        "System.Data": "",
        "System.Data.DataSetExtensions": "",
        "System.Security": "",
        "System.Xml": "",
        "System.Xml.Linq": "",
        "Microsoft.CSharp": ""
      }
    },
    "net40": {
      "compilationOptions": {
        "define": [ "SERIALIZABLE", "ENABLE_CRYPTO", "ENABLE_SNM" ]
      },
      "frameworkAssemblies": {
        "System.Core": "",
        "System.Data": "",
        "System.Data.DataSetExtensions": "",
        "System.Security": "",
        "System.Xml": "",
        "System.Xml.Linq": "",
        "Microsoft.CSharp": ""
      }
    },
    "net35": {
      "compilationOptions": {
        "define": [ "SERIALIZABLE", "ENABLE_CRYPTO", "ENABLE_SNM", "NET_3_5" ]
      },
      "frameworkAssemblies": {
        "System.Core": "",
        "System.Data": "",
        "System.Data.DataSetExtensions": "",
        "System.Security": "",
        "System.Xml": "",
        "System.Xml.Linq": ""
      }
    },
    ".NETPortable,Version=v4.5,Profile=Profile7": {
      "compilationOptions": {
        "define": [ "PORTABLE", "ENABLE_CRYPTO" ]
      },
      "frameworkAssemblies": {
        "System.Core": "",
        "System.Xml": "",
        "System.Xml.Linq": "",
        "Microsoft.CSharp": ""
      }
    },
    ".NETPortable,Version=v4.5,Profile=Profile111": {
      "compilationOptions": {
        "define": [ "PORTABLE", "ENABLE_CRYPTO" ]
      },
      "frameworkAssemblies": {
        "System.Core": "",
        "System.Xml": "",
        "System.Xml.Linq": "",
        "Microsoft.CSharp": ""
      }
    },
    "dotnet": {
      "compilationOptions": {
        "define": [ "COREFX" ],
        "allowUnsafe": true
      },
      "dependencies": {
        "System.Security.Cryptography.Algorithms": "4.0.0-beta-23225",
        "System.Security.Cryptography.Primitives": "4.0.0-beta-23225",
        "System.Text.Encoding.Extensions": "4.0.10",
        "System.Text.Encoding.CodePages": "4.0.0",
        "System.Text.Encoding": "4.0.10",
        "System.Net.NetworkInformation": "4.0.0",
        "System.Net.Primitives": "4.0.0",
        "System.Collections": "4.0.10",
        "System.Threading": "4.0.10",
        "System.Runtime": "4.0.20",
        "System.Linq": "4.0.0",
        "Microsoft.CSharp": "4.0.0"
      }
    }
  },

  "compileExclude": [ "packages.*.config", "MimeKit.*.project.json", "MacInterop\\*.cs" ]
}
